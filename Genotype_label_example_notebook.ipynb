    "def get_counts_by_gene_for_inherited_scenarios(td_input):\n",
    "    \n",
    "    # Add annotations with name I want\n",
    "    td_input = td_input.annotate_cols(Sample = td_input.id)\n",
    "    td_input = td_input.annotate_rows(Gene = td_input.gene)\n",
    "    \n",
    "    # Convert input trio dataset to a table\n",
    "    input_table = td_input.key_cols_by().entries()\n",
    "    \n",
    "    # Sum by gene & sample\n",
    "    counts = input_table.group_by(input_table.Sample, input_table.Gene).aggregate(\n",
    "        Chrom = hl.agg.take(input_table.locus.contig, 1)[0],\n",
    "        #Gene_ID = hl.agg.collect_as_set(input_table.Gene_ID),\n",
    "        site = hl.agg.sum(input_table.Site),\n",
    "        kid_call = hl.agg.sum(input_table.Call_Child),\n",
    "        kid_AC = hl.agg.sum(input_table.AC_Child),\n",
    "        dad_call = hl.agg.sum(input_table.Call_Dad),\n",
    "        dad_AC = hl.agg.sum(input_table.AC_Dad),\n",
    "        mom_call = hl.agg.sum(input_table.Call_Mom),\n",
    "        mom_AC = hl.agg.sum(input_table.AC_Mom),\n",
    "        alleles_from_dad = hl.agg.sum(input_table.Alleles_from_Dad),\n",
    "        alleles_from_mom = hl.agg.sum(input_table.Alleles_from_Mom),\n",
    "        alleles_indeterminate = hl.agg.sum(input_table.Alleles_Indeterminate) )\n",
    "   \n",
    "    # Apply logic:\n",
    "    #\n",
    "    # Mom has m >= 0 variants.  Dad has d >= 0 variants.   \n",
    "    # Kid inherits km >= 0 variants from mom and kd >= 0 variants from dad.\n",
    "    \n",
    "    # Default labels to missing\n",
    "    counts = counts.annotate(Proband_Label = 'PMS',\n",
    "                             Father_Label = 'FMS',\n",
    "                             Mother_Label = 'MMS')\n",
    "     \n",
    "    # If km > 0 and kd > 0, code the kid as homozygote non-reference\n",
    "    #      If m > km code the mom as homozygote non-reference\n",
    "    #           Otherwise code the mom as heterozygote\n",
    "    #      If d > kd code the dad as homozygote non-reference\n",
    "    #           Otherwise code the dad as heterozygote\n",
    "    counts = counts.annotate(Proband_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom > 0), 'PHV')\n",
    "                                .default(counts.Proband_Label),\n",
    "                             Father_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom > 0) &\n",
    "                                      (counts.dad_AC > counts.alleles_from_dad), 'FHV')\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom > 0), 'FHT')\n",
    "                                .default(counts.Father_Label),\n",
    "                             Mother_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom > 0) &\n",
    "                                      (counts.mom_AC > counts.alleles_from_mom), 'MHV')\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom > 0), 'MHT')\n",
    "                                .default(counts.Mother_Label)\n",
    "                            )\n",
    "    \n",
    "    # If km > 0 and kd = 0, code the kid as heterozygote non-reference\n",
    "    #      If m > km code the mom as homozygote non-reference\n",
    "    #           Otherwise code the mom as heterozygote\n",
    "    #      If d > 0 code the dad as heterozygote\n",
    "    #           Otherwise, if the dad has data at any of the k sites\n",
    "    #           code him as homozygous reference (if not, leave as missing)\n",
    "    counts = counts.annotate(Proband_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom > 0), 'PHT')\n",
    "                                .default(counts.Proband_Label),\n",
    "                             Father_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom > 0) &\n",
    "                                      (counts.dad_AC > 0), 'FHT')\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom > 0) &\n",
    "                                      (counts.dad_call > 0), 'FHR')\n",
    "                                .default(counts.Father_Label),\n",
    "                             Mother_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom > 0) &\n",
    "                                      (counts.mom_AC > counts.alleles_from_mom), 'MHV')\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom > 0), 'MHT')\n",
    "                                .default(counts.Mother_Label)\n",
    "                            )   \n",
    "    \n",
    "    # If km = 0 and kd > 0 code the kid as heterozygote\n",
    "    #      If m > 0 code the mom as heterozygote\n",
    "    #           Otherwise if the mom has data at any of the k sites\n",
    "    #           code her as homozygous reference (if not, leave as missing)\n",
    "    #      If d > kd code the dad as homozygote non-reference\n",
    "    #           Otherwise code the dad as heterozygote\n",
    "    counts = counts.annotate(Proband_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom == 0), 'PHT')\n",
    "                                .default(counts.Proband_Label),\n",
    "                             Father_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.dad_AC > counts.alleles_from_dad), 'FHV')\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom == 0), 'FHT')\n",
    "                                .default(counts.Father_Label),\n",
    "                             Mother_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.mom_AC > 0), 'MHT')\n",
    "                                .when((counts.alleles_from_dad > 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.mom_call > 0), 'MHR')\n",
    "                                .default(counts.Mother_Label)\n",
    "                            )   \n",
    "    \n",
    "    # If km = 0 and kd = 0, code the kid as homozygous reference if they have data at any site \n",
    "    # (if all sites are missing, leave as missing)\n",
    "    #      If d > 0, code dad as heterozygote   \n",
    "    #           Otherwise if any site is non-missing code him as homozygous reference\n",
    "    #      If m > 0, code mom as heterozygote\n",
    "    #           Otherwise if any site is non-missing code her as homozygous reference\n",
    "    counts = counts.annotate(Proband_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.kid_call > 0), 'PHR')\n",
    "                                .default(counts.Proband_Label),\n",
    "                             Father_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.dad_AC > 0), 'FHT')\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.dad_call > 0), 'FHR')\n",
    "                                .default(counts.Father_Label),\n",
    "                             Mother_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.mom_AC > 0), 'MHT')\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.mom_call > 0), 'MHR')\n",
    "                                .default(counts.Mother_Label)\n",
    "                            )\n",
    "\n",
    "    # Need to also handle indeterminate case; this happens at loci where everyone is het\n",
    "    #\n",
    "    # Parents will be marked as het using rules already applied, but the child needs to be handled\n",
    "    counts = counts.annotate(Proband_Label = hl.case()\n",
    "                                .when((counts.alleles_from_dad == 0) & (counts.alleles_from_mom == 0) &\n",
    "                                      (counts.alleles_indeterminate > 0), 'PHT')\n",
    "                                .default(counts.Proband_Label)\n",
    "                            )\n",
    "    \n",
    "    # If we infer the general rules that:\n",
    "    #      1) A child is homvar if T from both parents\n",
    "    #      2) A parent is homvar if T at least 1 allele to child with at least 1 additional allele\n",
    "    # Then the indeterminate cases cannot make someone homvar by themselves, because you can't definitively\n",
    "    # say who the alleles are from, so that rules already applied will have marked definitively homvar\n",
    "    # individuals\n",
    "    \n",
    "    # This combines the proband, father, and mother labels\n",
    "    counts = counts.annotate(Label = counts.Proband_Label + '_' + \n",
    "                                     counts.Father_Label + '_' +\n",
    "                                     counts.Mother_Label)\n",
    "    \n",
    "    return(counts)"
   ]
  },
