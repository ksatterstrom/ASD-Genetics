    "# Add counts for every allele inherited by child from father and mother,\n",
    "# not just those that would be counted in TDT\n",
    "def get_parent_aware_inherited_counts(td_input):\n",
    " \n",
    "    # Don't look at Y or M here\n",
    "    td_input = td_input.filter_rows( (td_input.locus.contig != 'chrY') & (td_input.locus.contig != 'chrM'), keep = True)\n",
    "\n",
    "    # Annotate type of site\n",
    "    td_input = td_input.annotate_entries(\n",
    "        Autosome_or_PAR = td_input.locus.in_autosome_or_par(),\n",
    "        XNonPAR_female = td_input.locus.in_x_nonpar() & (td_input.is_female == True),\n",
    "        XNonPAR_male = td_input.locus.in_x_nonpar() & (td_input.is_female == False))\n",
    "\n",
    "    # Annotate candidate genotype configurations\n",
    "    td_input = td_input.annotate_entries(has_candidate_GT = \n",
    "        ( td_input.Autosome_or_PAR & (\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_var()) |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_var()) |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_var()) |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) |\n",
    "                             \n",
    "        ( td_input.XNonPAR_female & (\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_var()) |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) |\n",
    "  \n",
    "        ( td_input.XNonPAR_male & (\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "            (td_input.proband_entry.GT.is_hom_ref() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_var()) |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "            (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) )\n",
    "\n",
    "    # If the GTs are out of whack, set them to missing    \n",
    "    td_input = td_input.annotate_entries(\n",
    "        proband_entry = td_input.proband_entry.annotate(\n",
    "            GT = hl.or_missing(td_input.has_candidate_GT, td_input.proband_entry.GT)),\n",
    "        father_entry = td_input.father_entry.annotate(\n",
    "            GT = hl.or_missing(td_input.has_candidate_GT, td_input.father_entry.GT)),\n",
    "        mother_entry = td_input.mother_entry.annotate(\n",
    "            GT = hl.or_missing(td_input.has_candidate_GT, td_input.mother_entry.GT))\n",
    "    )    \n",
    "    \n",
    "    # Now annotate with stats about the site\n",
    "    td_input = td_input.annotate_entries(   \n",
    "        # The site\n",
    "        Site = 1,\n",
    "    \n",
    "        # Proband\n",
    "        Call_Child = hl.if_else(hl.is_defined(td_input.proband_entry.GT), 1, 0), \n",
    "        \n",
    "        AC_Child = ( hl.case()\n",
    "            .when( (td_input.XNonPAR_male == False) & td_input.proband_entry.GT.is_het(), 1)\n",
    "            .when( (td_input.XNonPAR_male == False) & td_input.proband_entry.GT.is_hom_var(), 2)\n",
    "            .when( (td_input.XNonPAR_male == True) & td_input.proband_entry.GT.is_hom_var(), 1)\n",
    "            .default(0) ),\n",
    "                \n",
    "        # Father\n",
    "        Call_Dad = hl.if_else(hl.is_defined(td_input.father_entry.GT), 1, 0),   \n",
    "        \n",
    "        AC_Dad = ( hl.case()\n",
    "            .when( (td_input.locus.in_x_nonpar() == False) & td_input.father_entry.GT.is_het(), 1)\n",
    "            .when( (td_input.locus.in_x_nonpar() == False) & td_input.father_entry.GT.is_hom_var(), 2)\n",
    "            .when( (td_input.locus.in_x_nonpar() == True) & td_input.father_entry.GT.is_hom_var(), 1)\n",
    "            .default(0) ),\n",
    "              \n",
    "        # Mother\n",
    "        Call_Mom = hl.if_else(hl.is_defined(td_input.mother_entry.GT), 1, 0),   \n",
    "        \n",
    "        AC_Mom = ( hl.case()\n",
    "            .when( td_input.mother_entry.GT.is_het(), 1)\n",
    "            .when( td_input.mother_entry.GT.is_hom_var(), 2)\n",
    "            .default(0) ),       \n",
    "    \n",
    "        # Inherited allele counts\n",
    "        Alleles_from_Dad = hl.if_else(\n",
    "            (td_input.Autosome_or_PAR & (\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_var()) |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) | \n",
    "        \n",
    "            (td_input.XNonPAR_female & (\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_ref()) |\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) |\n",
    "\n",
    "            (td_input.XNonPAR_male & False), 1, 0),\n",
    "    \n",
    "        Alleles_from_Mom = hl.if_else(\n",
    "            (td_input.Autosome_or_PAR & (\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_var()) |\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_var()) |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_het()     & td_input.mother_entry.GT.is_hom_var()) |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) |\n",
    "  \n",
    "            (td_input.XNonPAR_female & (\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_het()     & td_input.father_entry.GT.is_hom_ref() & td_input.mother_entry.GT.is_hom_var()) |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_het())     |\n",
    "                (td_input.proband_entry.GT.is_hom_var() & td_input.father_entry.GT.is_hom_var() & td_input.mother_entry.GT.is_hom_var()) ) ) |\n",
    "  \n",
