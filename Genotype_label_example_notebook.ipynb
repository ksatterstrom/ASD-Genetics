    "# This function is to prepare the trio datasets for genotype labeling\n",
    "def prep_trio_dataset(td):\n",
    "    \n",
    "    # Apply rarity filter (note: different for smaller datasets)\n",
    "    td = td.filter_rows((hl.is_missing(td.gnomad_non_neuro_AF) | (td.gnomad_non_neuro_AF <= 0.001)) & \n",
    "                        ((td.AC / td.AN) <= 0.001), keep = True)\n",
    "    \n",
    "    # Indels can't be synonymous variants\n",
    "    td = td.annotate_rows(isSYN = hl.if_else(td.isIndel, False, td.isSYN))\n",
    "    \n",
    "    # In case of double counting, missense variants\n",
    "    td = td.annotate_rows(isMIS = hl.if_else((td.isPTV & td.isMIS), False, td.isMIS))\n",
    "    \n",
    "    # In case of double counting, synonymous variants\n",
    "    td = td.annotate_rows(isSYN = hl.if_else((td.isPTV & td.isSYN) | (td.isMIS & td.isSYN), False, td.isSYN))\n",
    "    \n",
    "    # Add MPC1 (MisA) and MPC2 (MisB) variant classes\n",
    "    td = td.annotate_rows(isMPC1 = (td.isMIS & (td.MPC >= 1) & (td.MPC < 2)), \n",
    "                          isMPC2 = (td.isMIS & (td.MPC >= 2)) )\n",
    "        \n",
    "    # Only keep variants I'll count\n",
    "    td = td.filter_rows(td.isPTV | td.isMIS | td.isSYN, keep = True)\n",
    "    \n",
    "    return(td)\n",
    "    "
   ]
